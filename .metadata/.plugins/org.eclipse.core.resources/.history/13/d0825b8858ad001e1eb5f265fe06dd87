    private void RetourBtnMouseClicked(MouseEvent evt) {
		new MainMenu().setVisible(true);
		this.dispose();
		
	}
 Connection Con = null;
PreparedStatement Pst=null;
ResultSet Rs = null;
Statement St = null;

            private void AfficherMembres() throws SQLException{
        Con= DriverManager.getConnection("jdbc:mysql://localhost:3306/librarydb","root","");
        St= Con.createStatement();
        Rs= St.executeQuery("select * from membre");
        TableMembre.setModel(DbUtils.resultSetToTableModel(Rs));
        TableMembre.addMouseListener(new java.awt.event.MouseAdapter() {
            public void mouseClicked(java.awt.event.MouseEvent evt) {
                TableMembreMouseClicked(evt);
            }
        });
        
    }
    
    
    private void EnregistrerMembreBtnActionPerformed(java.awt.event.ActionEvent evt) {                                                     
         if(NomMembre.getText().isEmpty() || PrenomMembre.getText().isEmpty() || ProfessionMembre.getSelectedIndex()==-1 || TelephoneMembre.getText().isEmpty() ||  
           PassMembre.getSelectedIndex()==-1){
           
           JOptionPane.showMessageDialog(this,"Informations manquantes !!");         
      }else{
           
           try{
               AfficherMembres();
               Con= DriverManager.getConnection("jdbc:mysql://localhost:3306/librarydb","root","");
               PreparedStatement Save = Con.prepareStatement("insert into membre(MembreNom, MembrePrenom, Profession, TypePass, MembreTelephone) values(?, ?, ?, ?, ?)");

               
               Save.setString(1, NomMembre.getText());
               Save.setString(2, PrenomMembre.getText());
               Save.setString(3, (String) ProfessionMembre.getSelectedItem());
               Save.setString(4, (String) PassMembre.getSelectedItem());
               Save.setString(5, (String) TelephoneMembre.getText());
               
               int row = Save.executeUpdate();
               JOptionPane.showMessageDialog(this,"Membre ajouté avec succés !");   
               Con.close();
               AfficherMembres();
              
          
           }catch(Exception ex) {
               
               JOptionPane.showMessageDialog(this,ex);   
           }
           
           
           
           
           
           
       }
    }   
    
    private void RetourBtnActionPerformed(java.awt.event.ActionEvent evt) {                                          
    	 new MainMenu().setVisible(true);
         this.dispose();
    }           	                              

    
    private void SupprimerMembreBtnActionPerformed(java.awt.event.ActionEvent evt) {                                                   
if(Key == -1) {
        	
        	JOptionPane.showMessageDialog(this,"Sélectionner un membre !!!");
        	
        }else {
        	try {
        		Con= DriverManager.getConnection("jdbc:mysql://localhost:3306/librarydb","root","");
        		String Query = "delete from membre where MembreID ="+Key;
        		Statement Del = Con.createStatement();
        		Del.executeUpdate(Query);
        		JOptionPane.showMessageDialog(this, "Membre supprimé avec succés !!!");
        		AfficherMembres();
        		
        	}catch(Exception e) {
        		
        	}
        		
        }
    }                                                  

    private void ModifierMembreBtnActionPerformed(java.awt.event.ActionEvent evt) {                                                  
        if (NomMembre.getText().isEmpty() || PrenomMembre.getText().isEmpty() || ProfessionMembre.getSelectedIndex()==-1  || TelephoneMembre.getText().isEmpty() || PassMembre.getSelectedIndex()==-1  ) {
            JOptionPane.showMessageDialog(this, "Information manquante !!");
        } else {
            try (Connection con = DriverManager.getConnection("jdbc:mysql://localhost:3306/librarydb", "root", "")) {
                String updateQuery = "update membre set MembreNom=?, MembrePrenom=?, Profession=? , TypePass=? , MembreTelephone=?  where MembreID=?";
                try (PreparedStatement save = con.prepareStatement(updateQuery)) {
                	 
        
                    save.setString(1, NomMembre.getText());
                    save.setString(2, PrenomMembre.getText());
                    save.setString(3, (String) ProfessionMembre.getSelectedItem());
                    save.setString(4, (String)PassMembre.getSelectedItem());
                    save.setString(5, (String) TelephoneMembre.getText());
                    save.setInt(6, Key);
                    
                   

                    int rowsAffected = save.executeUpdate();
                    if (rowsAffected > 0) {
                        AfficherMembres();
                        JOptionPane.showMessageDialog(this, "Membre mis à jour avec succès !!");
                    } else {
                        JOptionPane.showMessageDialog(this, "Aucune mise à jour effectuée. Vérifiez les informations fournies.");
                    }
                }
            } catch (SQLException e) {
                JOptionPane.showMessageDialog(this, "Erreur lors de la mise à jour du membre : " + e.getMessage());
            }
        }
    }                                                 

    
    private void PrenomMembreActionPerformed(java.awt.event.ActionEvent evt) {                                             
