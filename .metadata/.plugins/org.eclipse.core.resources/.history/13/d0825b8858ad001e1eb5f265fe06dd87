    }                                            
    int Key=-1;
    private void TableMembreMouseClicked(java.awt.event.MouseEvent evt) {                                         
    	 DefaultTableModel model = (DefaultTableModel) TableMembre.getModel();
 	    int MyIndex = TableMembre.getSelectedRow();
 	    Key = Integer.valueOf(model.getValueAt(MyIndex, 0).toString());
 	    NomMembre.setText(model.getValueAt(MyIndex, 1).toString());
 	    PrenomMembre.setText(model.getValueAt(MyIndex, 2).toString());
 	    ProfessionMembre.setSelectedItem(model.getValueAt(MyIndex, 3).toString());
 	    TelephoneMembre.setText(model.getValueAt(MyIndex, 4).toString());
 	    PassMembre.setSelectedItem(model.getValueAt(MyIndex, 5).toString());
    }                                        

   
